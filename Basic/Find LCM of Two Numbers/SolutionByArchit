#include<bits/stdc++.h>
using namespace std;

int lcm(int a,int b){
    int x;
    for(int i=1; i<=b;i++){
        if(a%i==0 && b%i==0){
            x=i;
        }
    }

    return (a*b)/x;
}

int main()
{

    int a,b;
    cin>>a>>b;
   
    int c;
    if(a>b)
    cout<<lcm(a,b);
    else
    {
        cout<<lcm(b,a);
    }
    
}
